    "@types/node": "^22.13.10",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.1.2",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.0",
    "react-router-devtools": "^1.1.10",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
