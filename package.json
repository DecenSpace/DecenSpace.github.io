    "@solana/wallet-adapter-wallets": "^0.19.36",
    "@solana/web3.js": "^1.98.2",
    "@tanstack/react-query": "^5.75.4",
    "bn.js": "^5.2.2",
    "buffer": "^6.0.3",
    "cesium": "^1.129.0",
    "dayjs": "^1.11.13",
    "fs": "^0.0.1-security",
    "http-server": "^14.1.1",
    "normalize.css": "^8.0.1",
    "path": "^0.12.7",
