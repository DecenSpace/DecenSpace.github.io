{
  "name": "decenspace.github.io",
  "version": "0.0.1",
  "description": "DecenSpace website",
  "scripts": {
    "postinstall": "npm run generate-icons",
    "build": "tsx build.ts",
    "dev": "tsx build.ts serve",
    "start": "http-server -p 3000 --proxy http://127.0.0.1:3000? ./dist",
    "test": "tsc -p . --noEmit",
    "analyze": "tsx build.ts --analyze",
    "generate-icons": "rimraf src/icons/* && svgr src/assets/icons --no-dimensions --typescript --out-dir src/icons --silent --no-index --no-prettier"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@craftamap/esbuild-plugin-html": "^0.8.0",
