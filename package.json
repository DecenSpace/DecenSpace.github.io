    "@types/react-dom": "^19.1.2",
    "dotenv": "^16.6.1",
    "esbuild": "^0.25.0",
    "react-router-devtools": "^1.1.10",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.2",
